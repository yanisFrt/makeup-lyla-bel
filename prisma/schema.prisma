generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Reservation {
  id           Int      @id @default(autoincrement())
  nom          String
  phone        String
  email        String?
  type_service String
  adresse      String
  date         String
  hour         String
  other_info   String?
  created_at   DateTime @default(now())
  update_date  DateTime @updatedAt
}
